::-webkit-scrollbar{
  width: 0;
  height: 0;
  color: transparent;
}
page{
  background: #f4f4f4;
  padding-bottom: 98rpx;
  padding-top: 90rpx;
}
.citypicker{
  position: fixed;
  height: 100%;
  width: 100%;
  min-height: 100%;
  background-color: red;
  z-index: 999;
}
.citypikcer-bg{
  position: fixed;
  top: 0;
  width: 100%;
  height: calc(100% - 400rpx);
}
picker-view{
  background: #fff;
  font-size: 36rpx;
  text-align: center;
}
.citybody {
  position: fixed;
  bottom: 0;
}
.cityheader {
  position: absolute;
  top: 0;
  width: 100%;
  z-index: 4;
  background: #fff;
}
.city-cancel {
  float: left;
  margin: 20rpx 20rpx 20rpx 40rpx;
  color: #818181;
}
.city-true {
  float: right;
  margin: 20rpx 40rpx 20rpx 20rpx;
  color: #2FB42E
}
.section .picker {
  background-color: #fff;
  border-bottom: 1px #d9d9d9 solid;
  border-top: 1px #d9d9d9 solid;
  padding: 20rpx;
}