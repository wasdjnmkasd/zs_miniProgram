.banner-1{
  width: 750rpx;
  height: 420rpx;
  text-align: center;
  background: #fff;
  position: relative;
}
.banner-1 image{
  width: 750rpx;
  height: 420rpx;
}
image.icon_mark{
  position: absolute;
  left: 0;
  top: 0;
  width: 140rpx;
  height: 140rpx;
}
image.icon_mark_right{
  position: absolute;
  top: 10rpx;
  right: 10rpx;
  width: 120rpx;
  height: 120rpx;
}