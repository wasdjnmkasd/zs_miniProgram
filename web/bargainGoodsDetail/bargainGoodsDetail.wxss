page{
  box-sizing: border-box;
  color: #333;
  padding: 0;
}
.bargainGoodsDetail-content{
  overflow: hidden;
  position: relative;
  padding: 0 25rpx 20rpx 25rpx;
}
.bargainGoodsDetail-content .bgImg{
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  z-index: -1;
}
.bargainGoodsDetail-content .content-header{
  height: 100rpx;
  line-height: 100rpx;
  overflow: hidden;
}
.bargainGoodsDetail-content .content-header .logo{
  float: left;
  color: #fff;
  font-size: 32rpx;
  position: relative;
  z-index: 9;
}
.bargainGoodsDetail-content .content-header .logo image{
  width: 40rpx;
  height: 40rpx;
  float: left;
  margin-top: 30rpx;
  margin-right: 10rpx;
}
.bargainGoodsDetail-content .content-header .rule{
  float: right;
  width: 120rpx;
  height: 40rpx;
  line-height: 40rpx;
  text-align: center;
  font-size: 24rpx;
  color: #fff;
  border: 1rpx solid #fff;
  border-radius: 40rpx;
  margin-top: 30rpx;
  position: relative;
  z-index: 9;
}
.bargainGoodsDetail-content .content-body .userMsg{
  text-align: center;
  position: absolute;
  left: 0;
  top: 40rpx;
  width: 100%; 
}
.bargainGoodsDetail-content .content-body .userMsg .imgBox{
  width: 80rpx;
  height: 80rpx;
  border-radius: 40rpx;
  overflow: hidden;
  border: 5rpx solid #fff;
  margin-left: calc(50% - 45rpx);
}
.bargainGoodsDetail-content .content-body .userMsg .imgBox image{
  width: 100%;
  height: 100%;
}
.bargainGoodsDetail-content .content-body .userMsg text{
  font-size: 30rpx;
  color: #999;
  margin-top: -5rpx;
  display: inline-block;
}
.bargainGoodsDetail-content .content-body .title{
  background: #fff;
  border-top-left-radius: 20rpx;
  border-top-right-radius: 20rpx;
  text-align: center;
  padding-top: 80rpx;
  padding-bottom: 10rpx;
  font-size: 38rpx;
}
.bargainGoodsDetail-content .content-body .title text{
  color: #999;
  font-size: 40rpx;
}
.bargainGoodsDetail-content .content-body .goodsDetail{
  background: #fff;
  padding: 0 20rpx 20rpx 20rpx;
  /* border-bottom-left-radius: 20rpx;
  border-bottom-right-radius: 20rpx; */
  overflow: hidden;  
}
.bargainGoodsDetail-content .content-body .goodsDetail > view{
  background: #f8f8f8;
  float: left;
}
.bargainGoodsDetail-content .content-body .goodsDetail > view.goodsDetail-left{
  width: 220rpx;
  height: 220rpx;
  border-top-left-radius: 10rpx;
  border-bottom-left-radius: 10rpx;
}
.bargainGoodsDetail-content .content-body .goodsDetail > view.goodsDetail-left image{
  width: 180rpx;
  height: 180rpx;
  margin: 20rpx;
}
.bargainGoodsDetail-content .content-body .goodsDetail > view.goodsDetail-right{
  width: calc(100% - 220rpx);
  height: 220rpx;
  border-top-right-radius: 10rpx;
  border-bottom-right-radius: 10rpx;
  padding-right: 20rpx;
  box-sizing: border-box;
  position: relative;
}
.bargainGoodsDetail-content .content-body .goodsDetail > view.goodsDetail-right .name{
  font-size: 32rpx;
  overflow: hidden;
  white-space: nowrap;
  text-overflow:ellipsis;
  margin-top: 20rpx;
}
.bargainGoodsDetail-content .content-body .goodsDetail > view.goodsDetail-right .price{
  color: #ff1104;
  font-size: 32rpx;
  font-weight: 700;
  margin-top: 10rpx;
}
.bargainGoodsDetail-content .content-body .goodsDetail > view.goodsDetail-right .time{
  position: absolute;
  left: 0;
  bottom: 30rpx;
  font-size: 32rpx;
  color: #ababab;
}
.bargainGoodsDetail-content .content-body .goodsDetail > view.goodsDetail-right .time text{
  display: inline-block;
  width: 48rpx;
  height: 48rpx;
  line-height: 48rpx;
  text-align: center;
  background: #333;
  color: #fff;
  border-radius: 5rpx;
  font-size: 28rpx;
  margin: 0 5rpx;
}
.bargainGoodsDetail-content .content-body .goodsDetail > view.goodsDetail-right .time text:last-child{
  margin-right: 10rpx;
}
.bargainGoodsDetail-content .progress{
  background: #fff;
  padding: 0 20rpx;
}
.bargainGoodsDetail-content .progress .showMsg{
  height: 50prx;
  line-height: 50rpx;
  font-size: 28rpx;
  color: #ff1104;
  background: #ffe665;
  display: inline-block;
  width: 200rpx;
  text-align:center;
  border-radius: 50rpx;
  margin-bottom: 20rpx;
  position: relative;
}
.bargainGoodsDetail-content .progress .showMsg::after{
  display:block;
  content:'';
  border-width:16rpx 16rpx 16rpx 16rpx;
  border-style:solid;
  border-color:#ffe665 transparent transparent transparent;
  position:absolute;
  left:50%;
  top:100%;
  margin-left: -16rpx;
}
.bargainGoodsDetail-content .progress progress{
  width: 100%;
  border: 2rpx solid #ff1104;
  border-radius: 20rpx;
}
.bargainGoodsDetail-content .progress .priceMsg{
  overflow: hidden;
  font-size: 26rpx;
  color: #999;
  margin-top: 5rpx;
}
.bargainGoodsDetail-content .progress .priceMsg .oldPrice{
  float: left;
  text-decoration:line-through;
}
.bargainGoodsDetail-content .progress .priceMsg .finalPrice{
  float: right;
}
.bargainGoodsDetail-content .btns{
  background: #fff;
  padding: 20rpx 0;
}
.bargainGoodsDetail-content .btns view:first-child,
.bargainGoodsDetail-content .btns button{
  margin: 0 20rpx;
  width: calc(100% - 40rpx);
  height: 88rpx;
  line-height: 88rpx;
  text-align: center;
  color: #fff;
  background: #fc2304;
  border-radius: 88rpx;
}
.bargainGoodsDetail-content .btns view:last-child{
  margin: 0 20rpx;
  width: calc(100% - 40rpx);
  height: 86rpx;
  line-height: 88rpx;
  text-align: center;
  color: #fc2304;
  border: 1rpx solid #fc2304;
  border-radius: 88rpx;
  margin-top: 20rpx;
}
.bargainGoodsDetail-content .list{
  background: #fff1f1;
  border-bottom-left-radius: 20rpx;
  border-bottom-right-radius: 20rpx;
  padding-bottom:20rpx;
}
.bargainGoodsDetail-content .list .list-title{
  height: 90rpx;
  line-height: 90rpx;
  text-align: center;
  font-size: 32rpx;
  font-weight: 700;
}
.bargainGoodsDetail-content .list .list-content-item{
  overflow: hidden;
  height: 80rpx;
  line-height: 80rpx;
}
.bargainGoodsDetail-content .list .list-content-item .item-index{
  float: left;
  width: 70rpx;
  text-align: center;
}
.bargainGoodsDetail-content .list .list-content-item .item-userMsg{
  float: left;
  overflow: hidden
}
.bargainGoodsDetail-content .list .list-content-item .item-price{
  float: right;
}
.bargainGoodsDetail-content .list .list-content-item .item-userMsg .userMgs-left{
  float: left;
  width: 60rpx;
  height: 60rpx;
  border-radius: 50%;
  overflow: hidden;
  margin-top: 10rpx;
  margin-right: 20rpx;
}
.bargainGoodsDetail-content .list .list-content-item .item-userMsg .userMgs-left image{
  width: 100%;
  height: 100%;
}
.bargainGoodsDetail-content .list .list-content-item .item-userMsg .userMgs-right{
  float: left;
  font-size: 28rpx;
  color: #333;
}
.bargainGoodsDetail-content .list .list-content-item .item-userMsg .userMgs-right > view{
  height: 35rpx;
  line-height: 35rpx;
}
.bargainGoodsDetail-content .list .list-content-item .item-userMsg .userMgs-right > view.name{
  margin-top: 10rpx;
}
.bargainGoodsDetail-content .list .list-content-item .item-userMsg .userMgs-right > view.text{
  color: #999;
  font-size: 24rpx;
  line-height: 30rpx;
}
.bargainGoodsDetail-content .list .list-content-item .item-price{
  font-size: 32rpx;
  font-weight: 700;
  margin-right: 30rpx;
}
.bargainGoodsDetail-content .list .list-content-item .item-price text{
  color: #ff1104;
}
.bargainGoodsDetail-content .alertBg{
  position: fixed;
  width: 100%;
  height: 100%;
  background: rgba(57,57,57,0.6);
  top: 0;
  left: 0;
  color: #333;
  display: none;
  z-index: 10;
}
.bargainGoodsDetail-content .alertBg.active{
  display: block;
}
.bargainGoodsDetail-content .alertBg .alertContent{
  width: 540rpx;
  height: 640rpx;
  background: #fff;
  border-radius: 20rpx;
  margin: 240rpx 0 0 105rpx;
}
.bargainGoodsDetail-content .alertBg .alertContent .title{
  height: 70rpx;
  line-height: 70rpx;
  text-align: center;
  font-size: 34rpx;
}
.bargainGoodsDetail-content .alertBg .alertContent .text{
  padding: 0 40rpx;
  font-size: 32rpx;
  margin-top: 10rpx;
  word-break: break-all;
  overflow: scroll;
  height: 530rpx;
}
.bargainGoodsDetail-content .alertBg .alertContent .text view{
  word-break: break-all;
  line-height: 45rpx;
}
.bargainGoodsDetail-content .alertBg .alertContent .cancelBtn{
  width: 70rpx;
  height: 70rpx;
  line-height: 70rpx;
  text-align: center;
  border: 1rpx solid #fff;
  border-radius: 50%;
  color: #fff;
  margin: 70rpx 0 0 230rpx;
}